{
  "name": "super-mapscraper-frontend",
  "version": "1.0.0",
  "description": "Frontend for Super MapScraper",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:esbuild && npm run build:closure",
    "build:esbuild": "esbuild src/index.js --bundle --format=esm --outfile=dist/bundle.esm.js --minify --sourcemap",
    "build:closure": "npx google-closure-compiler --js dist/bundle.esm.js --js_output_file dist/bundle.min.js --compilation_level ADVANCED --language_out ES2020",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "preact": "^10.19.3",
    "preact-router": "^4.1.2",
    "goober": "^2.1.13",
    "leaflet": "^1.9.4",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "vite": "^5.0.8",
    "esbuild": "^0.19.8",
    "google-closure-compiler": "^20231120.0.0",
    "vitest": "^1.0.4",
    "@preact/preset-vite": "^2.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}